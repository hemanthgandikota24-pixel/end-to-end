{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "endtoendhemanth"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"endtoendsql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'endtoendsql'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://endtoendhemanth.dfs.core.windows.net/"
		},
		"endtoendsql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "endtoend12.database.windows.net"
		},
		"endtoendsql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "endtoend"
		},
		"endtoendsql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsql"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "endtoendhemanth_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"endtoendsql_password": {
						"value": "[parameters('endtoendsql_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"endtoendsql_properties_typeProperties_server": {
						"value": "[parameters('endtoendsql_properties_typeProperties_server')]"
					},
					"endtoendsql_properties_typeProperties_database": {
						"value": "[parameters('endtoendsql_properties_typeProperties_database')]"
					},
					"endtoendsql_properties_typeProperties_userName": {
						"value": "[parameters('endtoendsql_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}